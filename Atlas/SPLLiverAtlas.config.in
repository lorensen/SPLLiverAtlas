#
# Atlas config file for SampleAtlas
# After processing by cmake, SampleAtlas.config be created
# All file prefixes are suggested for consistency with other Open Atlases
# Required:
AtlasName: SampleAtlas
# Required: These three files must exist
VolumeFileName: @SampleAtlas_SOURCE_DIR@/Atlas/SampleAtlasVolume.nrrd
LabelFileName: @SampleAtlas_SOURCE_DIR@/Atlas/SampleAtlasLabels.nrrd
ColorTableFileName: @SampleAtlas_SOURCE_DIR@/Atlas/SampleAtlasColors.ctbl
# Generated by GenerateAdjacencies
AdjacenciesFileName: @SampleAtlas_SOURCE_DIR@/Models/adjacencies.txt
# Generated by GenerateStatisticsFromLabels
StatisticsDirectory: @SampleAtlas_SOURCE_DIR@/Statistics
# Generated by GenerateCubesFromLabels and GenerateModelsFromLabels
ModelsDirectory: @SampleAtlas_SOURCE_DIR@/Models
VTKDirectory: @SampleAtlas_SOURCE_DIR@/Models/VTK
STLDirectory: @SampleAtlas_SOURCE_DIR@/Models/STL
MRMLDirectory: @SampleAtlas_BINARY_DIR@/MRML
# Generated by DisplayAllAdjacencies
ScreenshotDirectory: @SampleAtlas_SOURCE_DIR@/Models/Screenshot
# Generated by GenerateDiffFromHead and GenerateDiffsFZromLastTwo
ChangesDirectory: @SampleAtlas_SOURCE_DIR@/Changes
# Generated by ReportAllDisconnectedRegions
FiducialsDirectory: @SampleAtlas_BINARY_DIR@/Fiducials
# Optional: Only needed if a github repo exists
GitDirectory: @SampleAtlas_SOURCE_DIR@
GitURL: https://github.com/USER/SampleAtlas
# Optional; Only needed if a github wiki exists
GitWikiDirectory: @SampleAtlas_SOURCE_DIR@/wiki
